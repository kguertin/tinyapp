<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if(userID){ %>
    <div class="display-userID">
      <form method="POST" action="/logout">
        <h5 style="margin:28px 20px 0 0;float: left;">Logged In As: <%= userID.email %></h5>
        <button class="btn btn-lg btn-outline-dark" style="float: right; margin: 20px 0;">Logout</button>
      </form>
    </div>
    <% } else{%>
    <div>
      <a href="http://localhost:8080/login" class="btn btn-lg btn-outline-light" style="margin: 20px 0;">Login</a>
      <a href="http://localhost:8080/register" class="btn btn-lg btn-outline-light" style="margin: 20px 0;">Register</a>
    </div>
    <% } %>
  </nav>
  <div style="clear: both"></div>
</header>